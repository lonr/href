{
  "name": "@lonr/href",
  "version": "1.0.1",
  "description": "href is a lib for working with incomplete URLs (without a base) in the builtin URL's way",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "/dist/index.*"
  ],
  "author": {
    "name": "lonr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lonr/href.git"
  },
  "homepage": "https://github.com/lonr/href",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.ts",
    "check:format": "prettier --list-different \"./**/*.{ts,js,json,md,yml}\"",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\"",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "test": "jest",
    "postinstall": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "URL",
    "url",
    "href",
    "query",
    "path"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^5.0.9",
    "is-ci": "^2.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "webpack": "^5.20.0",
    "webpack-cli": "^4.5.0",
    "whatwg-url": "^8.4.0"
  },
  "dependencies": {}
}
